{
  "name": "vue-fuse",
  "version": "4.0.0-beta0",
  "description": "A Vue.js pluggin for fuzzy search library, Fuse.js",
  "author": "Shayne O'Sullivan <shayneosull@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/shayneo/vue-fuse",
  "keywords": [
    "vue",
    "vue fuse",
    "fuse.js",
    "fuse",
    "search",
    "fuzzy search",
    "vue search",
    "fuzzy search vue"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shayneo/vue-fuse.git"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "serve": "vite preview"
  },
  "files": [
