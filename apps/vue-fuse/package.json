      "require": "./dist/vue-fuse.umd.js"
    }
  },
  "dependencies": {
    "vue-demi": "latest"
  },
  "devDependencies": {
    "@types/node": "^16.10.3",
    "@vitejs/plugin-vue": "^1.9.2",
    "fuse.js": "^6.4.6",
    "typescript": "^4.4.3",
    "vite": "^2.6.0",
    "vue": "^3.2.20",
    "vue-tsc": "^0.3.0"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-rc.1",
    "fuse.js": "^6.4.6",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
